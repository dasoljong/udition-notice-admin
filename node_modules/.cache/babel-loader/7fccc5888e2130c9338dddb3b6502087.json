ter(\"modules/imageResize\", ImageResize);\n\nexport default {\n  props: {\n    text: {\n      type: String,\n      required: true\n    }\n  },\n  data() {\n    return {\n      editorOption: {\n        modules: {\n          toolbar: toolbarOptions,\n          imageDrop: true,\n          imageResize: true\n        }\n      }\n    };\n  },\n  methods: {\n    onEditorChange() {\n      console.log(this.content);\n      this.$emit(\"update-content\", this.content);\n    }\n  }\n};\n</script>\n\n<style>\n.ql-editor strong em u {\n  font-style: italic;\n  font-weight: bold;\n  border-bottom: 1px solid black;\n}\n.ql-editor strong em {\n  font-style: italic;\n  font-weight: bold;\n}\n.ql-editor strong u {\n  font-weight: bold;\n  border-bottom: 1px solid black;\n}\n.ql-editor em u {\n  font-style: italic;\n  border-bottom: 1px solid black;\n}\n.ql-editor strong {\n  font-weight: bold;\n}\n.ql-editor em {\n  font-style: italic;\n}\n.ql-editor u {\n  border-bottom: 1px solid black;\n}\n.quill-editor {\n  height: 500px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}